<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="kr10_r1100_2">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import kr10_r1100_2 urdf file -->
    <xacro:include filename="$(find kuka_agilus_support)/urdf/kr10_r1100_2.urdf.xacro" />

    <!-- Import control_xacro -->
    <xacro:include filename="kr10_r1100_2.ros2_control.xacro" />


    <xacro:kr10_r1100_2_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
